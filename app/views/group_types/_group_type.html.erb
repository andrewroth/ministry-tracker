<tr id="<%= dom_id(group_type) %>">
    <td><%=h group_type.group_type %></td>
    <td style="text-align: center;">
        <%- if group_type.mentor_priority? -%>
        <%= image_tag('silk/accept.png') %>
        <%- end %>
    </td>
    <td style="text-align: center;">
        <%- if group_type.public? -%>
        <%= image_tag('silk/accept.png') %>
        <%- end %>
    </td>
    <%- unless Cmt::CONFIG[:disable_group_timetable_impact] == true -%>
    	<td><%=h group_type.unsuitability_metric(group_type.unsuitability_leader) %></td>
    	<td><%=h group_type.unsuitability_metric(group_type.unsuitability_coleader) %></td>
    	<td><%=h group_type.unsuitability_metric(group_type.unsuitability_participant) %></td>
    <%- end -%>
    <td><%= link_to_remote(image_tag('edit.png', :alt => "Edit this type", :title => "Edit this type"), 
                                                 :url => edit_group_type_path(group_type),
                                                 :before => "$('#spinnergt').show()",
                        											 	 :method => :get) %></td>
    <td><%= link_to_remote(image_tag('delete.png', :alt => "Delete this type", :title => "Delete this type"), 
                                                   :url => group_type, 
                                                   :confirm => "Are you sure you want to remove the #{group_type.group_type} group type?", 
                                                   :method => :delete,
                                                   :before => "$('#spinnergt').show()") %></td>
    <td><%= spinner(dom_id(group_type)) %></td>
 </tr>
