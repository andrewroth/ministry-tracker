
  <ul id='profile_mentees'>
  	<%- mentees = Person.find_all_by_person_mentor_id(@person.id) %>
    <%- mentees.each do |mentee| 
    -%>
          <%= render :partial => 'profile_mentee_li', :locals => { :mentee => mentee } %>				
    <%- end -%>
		<%- if mentees.size == 0 %>
		  No mentee(s) currently
		<%- end -%>
		  <div class="floatLeft"><%= spinner('m2') %></div>
  <div id="progress-bar"></div>
  </ul>
  <br>
  <div id="search_mentee">
    <%= render :partial => "mentee_search_box", :locals => { :person => @person, :q2 => @q2 } %> 
  </div> 