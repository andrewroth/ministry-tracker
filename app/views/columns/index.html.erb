<h1>Listing columns</h1>

<table>
  <tr>
  </tr>
  
<% for column in @columns %>
  <tr>
    <td><%= column.title %></td>
    <td><%= link_to 'Edit', edit_column_path(column) %></td>
    <td><%= link_to 'Destroy', column_path(column), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New column', new_column_path %>
