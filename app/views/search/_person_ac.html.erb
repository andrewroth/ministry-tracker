<%= Rails.cache.fetch([person_ac, person_ac.timetable_id.present?, person_ac.ministries_concat, person_ac.campuses_concat, person_ac.email, @ac_actions, @ac_title]) do
  
  result = "<div class='autoCompleteResult' title='#{@ac_title}'>" +
           "<div class='acLinkPath' style='display: none;'>/people/#{person_ac.id}</div>" +
           "<div class='autoCompletePic'>#{ac_profile_picture_for_person(person_ac)}</div>" +

           "<div class='autoCompleteInfo' person_id='#{person_ac.id}' person_fname='#{person_ac.first_name}' person_lname='#{person_ac.last_name}' email='#{person_ac.email.downcase}'>" +
           "<div class='autoCompleteName'>#{person_ac.first_name.capitalize} #{person_ac.last_name.capitalize}</div>"

  if person_ac.timetable_id.present? && @ac_actions
    result += "<div class='autoCompleteIconLink'>" +
              link_to("<div class='autoCompleteIconTimetable'>&nbsp;</div>", "/people/#{person_ac.id}/timetables/#{person_ac.timetable_id}", :title => "Goto #{person_ac.first_name.capitalize}&rsquo;s timetable") +
              "&nbsp;</div>"
  end

  result += "<div class='autoCompleteExtraInfo'>#{ac_info_for_person(person_ac, @ac_actions)}</div>" +
            "</div>" +
            "</div>"
  
  result
  
end %>